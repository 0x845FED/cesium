    "merge-stream": "^2.0.0",
    "mime": "^2.0.3",
    "mkdirp": "^1.0.0",
    "open": "^7.0.0",
    "request": "^2.79.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.21.4",
    "rollup-plugin-external-globals": "^0.5.0",
